{
    "extends": [
      "react-app"
    ],
    "plugins": ["react", "@typescript-eslint", "prettier"],
    "env": {
      "browser": true,
      "jasmine": true,
      "jest": true
    },
    "rules": {
        "import/no-anonymous-default-export": "off"
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
    "parser": "@typescript-eslint/parser"
}